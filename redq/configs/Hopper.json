{
  "env_name": "HalfCheetah-v2",
  "buffer":{
    "max_buffer_size": 1000000
  },
  "agent":{
    "gamma": 0.99,
    "update_target_network_interval": 20,
    "target_smoothing_tau": 0.2,
    "q_network":{
      "n": 20,
      "hidden_dims": [128,128],
      "optimizer_class": "Adam",
      "learning_rate":0.00005,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "hidden_dims": [128,128],
      "optimizer_class": "Adam",
      "deterministic": false,
      "learning_rate":0.00005,
      "act_fn": "relu",
      "out_act_fn": "identity"
    }
  },
  "trainer":{
    "max_episode": 10000,
    "num_updates_per_iteration":10,
    "batch_size": 256,
    "max_trajectory_length":1000,
    "test_interval": 10,
    "num_test_trajectories": 5,
    "render": true
  },
  "env":{
    "reward_scale": 5.0
  }
  
}
