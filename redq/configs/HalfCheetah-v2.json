{
  "env_name": "HalfCheetah-v2",
  "buffer":{
    "max_buffer_size": 1000000
  },
  "agent":{
    "gamma": 0.99,
    "update_target_network_interval": 2,
    "target_smoothing_tau": 0.01,
    "num_q_networks": 10,
    "num_q_samples": 2,
    "q_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "deterministic": false,
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "entropy":{
      "automatic_tuning": true,
      "learning_rate": 0.0001,
      "optimizer_class": "Adam"
    }
  },
  "trainer":{
    "max_iteration": 1000000,
    "num_updates_per_iteration":1,
    "batch_size": 256,
    "max_trajectory_length":1000,
    "test_interval": 10,
    "num_test_trajectories": 5,
    "render": true,
    "start_timestep": 1000
  },
  "env":{
    "reward_scale": 5.0
  }
  
}
