{
  "env_name": "Humanoid-v2",
  "buffer":{
    "max_buffer_size": 1000000
  },
  "agent":{
    "gamma": 0.99,
    "q_network":{
      "hidden_dims": [128,128],
      "optimizer_class": "Adam",
      "learning_rate":0.0001,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "v_network":{
      "hidden_dims": [128,128],
      "optimizer_class": "Adam",
      "learning_rate":0.0001,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "hidden_dims": [128,128],
      "optimizer_class": "Adam",
      "deterministic": false,
      "learning_rate":0.0001,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "entropy":{
      "automatic_tuning": true,
      "learning_rate": 0.0001,
      "optimizer_class": "Adam"
    }
  },
  "trainer":{
    "update_v_target_interval": 10,
    "max_episode": 20000,
    "num_updates_per_iteration":20,
    "batch_size": 64,
    "max_trajectory_length":500,
    "test_interval": 10,
    "num_test_trajectories": 5,
    "target_smoothing_tau": 0.001
  },
  "env":{
    "reward_scale": 5.0
  }
  
}
