{
  "env_name": "Humanoid-v2",
  "agent":{
    "q_network":{
      "hidden_dims": [128,128],
      "optimizer_class": "Adam",
      "learning_rate":0.0005
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "v_network":{
      "hidden_dims": [128,128],
      "optimizer_class": "Adam",
      "learning_rate":0.0005,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "width": [128,128],
      "optimizer_class": "Adam",
      "deterministic": false,
      "learning_rate":0.0005,
      "act_fn": "relu",
      "out_act_fn": "identity"
    }
  },
  "trainer":{
    "update_v_target_interval": 10,
    "max_iteration": 1000000,
    "update_per_iteration":20,
    "batch_size": 32,
    "max_trajectory_length":500,
    "test_interval": 1000,
    "num_test_trajectories": 5
  }
  
}
