{
  "env_name": "Hopper-v3",
  "buffer":{
    "max_buffer_size": 1000000
  },
  "agent":{
    "gamma": 0.99,
    "update_target_network_interval": 2,
    "target_smoothing_tau": 0.01,
    "q_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "deterministic": false,
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "entropy":{
      "automatic_tuning": true,
      "learning_rate": 0.000005,
      "optimizer_class": "Adam"
    }
  },
  "trainer":{
    "max_iteration": 3000,
    "num_updates_per_iteration":1000,
    "num_steps_per_iteration": 1000,
    "batch_size": 256,
    "max_trajectory_length":1000,
    "test_interval": 5,
    "num_test_trajectories": 3,
    "save_model_interval": 100,
    "start_timestep": 1000,
    "save_video_demo_interval": 100,
    "log_interval": 1
  },
  "env":{
    "reward_scale": 10.0
  }
  
}
