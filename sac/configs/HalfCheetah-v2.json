{
  "env_name": "HalfCheetah-v2",
  "buffer":{
    "max_buffer_size": 1000000
  },
  "agent":{
    "gamma": 0.99,
    "update_target_network_interval": 1,
    "target_smoothing_tau": 0.005,
    "q_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "deterministic": false,
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity",
      "re_parameterize": true
    },
    "entropy":{
      "automatic_tuning": true,
      "learning_rate": 0.0003,
      "optimizer_class": "Adam"
    }
  },
  "trainer":{
    "max_iteration": 5000000,
    "num_updates_per_iteration":1,
    "num_steps_per_iteration": 1,
    "batch_size": 256,
    "max_trajectory_length":1000,
    "test_interval": 1000,
    "num_test_trajectories": 5,
    "save_model_interval": 100000,
    "start_timestep": 5000,
    "save_video_demo_interval": 100000,
    "log_interval": 1000
  },
  "env":{
    "reward_scale": 10.0
  }
  
}
