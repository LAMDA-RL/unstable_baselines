{
  "env_name": "Hopper-v3",
  "buffer":{
    "max_buffer_size": 2048
  },
  "agent":{
    "gamma": 0.99,
    "q_network":{
      "hidden_dims": [64,64],
      "optimizer_class": "Adam",
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "hidden_dims": [64,64],
      "optimizer_class": "Adam",
      "deterministic": false,
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity"
    }
  },
  "trainer":{
    "max_iteration": 3000,
    "num_updates_per_iteration" :5,
    "num_steps_per_iteration": 2048,
    "batch_size": 64,
    "max_trajectory_length":1000,
    "test_interval": 5,
    "num_test_trajectories": 3,
    "save_model_interval": 100,
    "start_timestep": 1000,
    "save_video_demo_interval": 100,
    "log_interval": 1
  },
  "env":{
    "reward_scale": 1.0
  }
  
}
